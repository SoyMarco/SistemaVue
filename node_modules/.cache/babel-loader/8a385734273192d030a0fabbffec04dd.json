one='';\r\n  this.editedItem.nombre='';\r\n  this.editedItem.descripcion='';\r\n        this.dialog = false;\r\n       /*  this.$nextTick(() => {\r\n          this.editedItem = Object.assign({}, this.defaultItem);\r\n          this.editedIndex = -1;\r\n        }); */\r\n      },\r\n\r\n    closeDelete() {\r\n      this.dialogDelete = false;\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      });\r\n    },\r\n\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}